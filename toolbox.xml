<xml id="toolbox" style="display: none;">
  <category name="Elements" colour="20">
    <block type="cycle_container"></block>
    <block type="cycle_text">
      <value name="TEXT">
        <block type="text">
          <field name="TEXT">Hello</field>
        </block>
      </value>
    </block>
    <block type="cycle_button">
      <statement name="CHILDREN">
        <block type="cycle_text">
          <value name="TEXT">
            <block type="text">
              <field name="TEXT">Click me!</field>
            </block>
          </value>
          <next>
            <block type="mousedown">
              <statement name="blocks">
                <block type="text_print">
                  <value name="TEXT">
                    <block type="text">
                      <field name="TEXT">I've been clicked!</field>
                    </block>
                  </value>
                </block>
              </statement>
            </block>
          </next>
        </block>
      </statement>
    </block>
    <block type="cycle_link">
      <statement name="CHILDREN">
        <block type="cycle_html_property">
          <value name="KEY">
            <block type="text">
              <field name="TEXT">target</field>
            </block>
          </value>
          <value name="VALUE">
            <block type="text">
              <field name="TEXT">_blank</field>
            </block>
          </value>
          <next>
            <block type="cycle_html_property">
              <value name="KEY">
                <block type="text">
                  <field name="TEXT">href</field>
                </block>
              </value>
              <value name="VALUE">
                <block type="text">
                  <field name="TEXT">https://google.com</field>
                </block>
              </value>
              <next>
                <block type="cycle_text">
                  <value name="TEXT">
                    <block type="text">
                      <field name="TEXT">Link to google</field>
                    </block>
                  </value>
                </block>
              </next>
            </block>
          </next>
        </block>
      </statement>
    </block>
    <block type="cycle_image">
      <statement name="CHILDREN">
        <block type="cycle_html_property">
          <value name="KEY">
            <block type="text">
              <field name="TEXT">src</field>
            </block>
          </value>
          <value name="VALUE">
            <block type="text">
              <field name="TEXT">http://i.imgur.com/GCKBxrv.gif</field>
            </block>
          </value>
        </block>
      </statement>
    </block>
    <block type="cycle_input">
      <statement name="CHILDREN">
        <block type="variables_set">
          <field name="VAR">inputText</field>
          <value name="VALUE">
            <block type="text">
              <field name="TEXT"></field>
            </block>
          </value>
          <next>
            <block type="cycle_html_property">
              <value name="KEY">
                <block type="text">
                  <field name="TEXT">value</field>
                </block>
              </value>
              <value name="VALUE">
                <block type="variables_get">
                  <field name="VAR">inputText</field>
                </block>
              </value>
              <next>
                <block type="keyup">
                  <statement name="blocks">
                    <block type="variables_set">
                      <field name="VAR">inputText</field>
                      <value name="VALUE">
                        <block type="objects_get">
                          <value name="OBJECT">
                            <block type="objects_get">
                              <value name="OBJECT">
                                <block type="variables_get">
                                  <field name="VAR">event</field>
                                </block>
                              </value>
                              <value name="KEY">
                                <block type="text">
                                  <field name="TEXT">target</field>
                                </block>
                              </value>
                            </block>
                          </value>
                          <value name="KEY">
                            <block type="text">
                              <field name="TEXT">value</field>
                            </block>
                          </value>
                        </block>
                      </value>
                    </block>
                  </statement>
                </block>
              </next>
            </block>
          </next>
        </block>
      </statement>
    </block>
    <block type="cycle_iframe">
      <statement name="CHILDREN">
        <block type="cycle_html_property">
          <value name="KEY">
            <block type="text">
              <field name="TEXT">src</field>
            </block>
          </value>
          <value name="VALUE">
            <block type="text">
              <field name="TEXT">http://stevekrouse.github.io/cycle</field>
            </block>
          </value>
        </block>
      </statement>
    </block>
    <!--list-->
    <!--list element (numbered/bullet/nothing)-->
    <!--heading-->
    <!--text area-->
    <!--table-->
    <!--table row-->
    <!--table cell-->
    <!--code (pre)-->
    <!--embed-->
